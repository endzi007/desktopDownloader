{
  "name": "ng-youtube-downloader",
  "version": "1.0.0",
  "description": "Youtube mp3, mp4 multiple downloader",
  "main": "src/main/index.js",
  "homepage": "./",
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder"
  },
  "keywords": [],
  "author": "Enis Jašarović",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "app-root-dir": "^1.0.2",
    "attr-accept": "^1.1.3",
    "dotenv": "^6.2.0",
    "electron-redux": "^1.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^7.0.1",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "webpack": "^4.26.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.4",
    "electron-webpack": "^2.6.1",
    "uglifyjs-webpack-plugin": "^2.1.2"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "custom.webpack.additions.js"
    },
    "renderer": {
      "webpackConfig": "custom.webpack.additions.js"
    }
  },
  "build": {
    "appId": "com.decom.ytdownload",
    "productName": "NG YouTube Downloader",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "directories": {
      "output": "build"
    }
  },
  "nsis": {
    "allowToChangeInstallationDirectory": true,
    "oneClick": false
  }
}
